<html t:type="layout" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">

	<t:form t:method="POST" t:id="fStudentPassChange">
		<table align="center">
			<tr>
				<td>
					Current password:
				</td>
				<td>
					<t:passwordfield t:id="stdPassword" t:value="stdPassword" />
				</td>
			</tr>
			<tr>
				<td>
					New password:
				</td>
				<td>
					<t:passwordfield t:id="newPassword" t:value="newPassword" />
				</td>
			</tr>
			<tr>
				<td>
					Retype new password:
				</td>
				<td>
					<t:passwordfield t:id="newPasswordRetype" t:value="newPasswordRetype" />
				</td>
			</tr>
			<tr>
				<td>

				</td>
				<td>
					<t:submit t:id="submitSave" value="Change password" />&nbsp;
				</td>
			</tr>
		</table>
		<t:errors />
	</t:form>
</html>